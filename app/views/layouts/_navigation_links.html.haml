- if user_signed_in?
  %li= link_to '新規作成', new_entry_path, :class => 'navbar-link'
  %li= link_to '通知', infos_path, :class => 'navbar-link'
  %li= link_to current_user.name, user_path(current_user), :class => 'navbar-link'
  %li= link_to 'プロフィール編集', edit_user_registration_path, :class => 'navbar-link'
  %li= link_to 'ログアウト', destroy_user_session_path, method: :delete, :class => 'navbar-link'
- else
  %li= link_to '登録', new_user_registration_path, :class => 'navbar-link'
  %li= link_to 'ログイン', new_user_session_path, :class => 'navbar-link'
